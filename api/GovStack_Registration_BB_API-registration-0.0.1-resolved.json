	"paths": {
	

		"/api/process-definition/{key}/start-with-form": {
			"post": {
				"tags": ["application-registration"],
				"summary": "Start application process instance with form",
				"description": "Creates application process instance",
				"operationId": "createProcessInstanceWithForm",
				"parameters": [
				  {
            "$ref": "#/components/parameters/Information-Mediator-Client"
          },
				  
				  {
					"name": "key",
					"description": "I think this key is issued by the creator app and = id, ",
					"in": "path",
					"required": true,
					"schema": {
						"type": "string",
						"example": "81c4445c-bff6-11ed-afa1-0242ac120002"
					}
				}],
				"requestBody": {
					"content": {
						"application/json": {
							"schema": {
								"$ref": "#/components/schemas/FormDataDto"
							}
						}
					},
					"required": true
				},
				"responses": {
					"500": {
						"description": "Internal server error",
						"content": {
							"*/*": {
								"schema": {
									"$ref": "#/components/schemas/SystemErrorDto"
								}
							}
						}
					},
					"200": {
						"description": "Returns started application process instance",
						"content": {
							"*/*": {
								"schema": {
									"$ref": "#/components/schemas/StartProcessInstanceResponse"
								}
							}
						}
					},
					"404": {
						"description": "Business process definition hasn't found",
						"content": {
							"*/*": {
								"schema": {
									"$ref": "#/components/schemas/SystemErrorDto"
								}
							}
						}
					}
				}
			}
		}




		
	},
